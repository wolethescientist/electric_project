    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.4",
    "rimraf": "^6.0.1",
    "serve": "^14.2.4",
    "tailwindcss": "^4",
